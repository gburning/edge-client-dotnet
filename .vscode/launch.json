        {
            "name": "Run Tests (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/test/NabtoClient.Tests/bin/Debug/net7.0/NabtoClient.Tests.dll",
            "args": [],
            "cwd": "${workspaceFolder}/test/NabtoClient.Tests",
            "console": "internalConsole",
